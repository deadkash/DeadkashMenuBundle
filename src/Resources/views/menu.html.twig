{% if items %}
    <ul>
        {% for item in items %}
            <li>
                <a href="{{ path(item.routeName) }}">{{ item.title }}</a>
                {% if item.subitems %}
                    <ul>
                        {% for subitem in item.subitems %}
                            <li><a href="{{ path(subitem.routeName) }}">{{ subitem.title }}</a></li>
                        {% endfor %}
                    </ul>
                {% endif %}
            </li>
        {% endfor %}
    </ul>
{% endif %}